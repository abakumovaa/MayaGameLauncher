<Window x:Class="MayaGameLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MayaGameLauncher"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1300" MinHeight="600" MinWidth="1024" WindowStyle="None"
WindowStartupLocation="CenterScreen">
    <Window.Background>
        <ImageBrush ImageSource="/Sources/back.jpg"/>
    </Window.Background>
    <Grid>
        <Frame Name="MainFrame"
       Source="Pages/LoadingPage.xaml"
       NavigationUIVisibility="Hidden"
       VerticalAlignment="Center"
       HorizontalAlignment="Center"
       />
    </Grid>
</Window>
